<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
    <!-- <th>Password digest</th> -->
    <th>Password confirmation</th>
    <th>Name</th>
    <th>Business name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Website</th>
    <th>Phone</th>
    <th>About</th>
    <th>Business</th>
    <th>Tag</th>
    <!-- <th>Favorite</th> -->
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.password %></td>
    <!-- <td><#%= user.password_digest %></td> -->
    <td><%= user.password_confirmation %></td>
    <td><%= user.name %></td>
    <td><%= user.business_name %></td>
    <td><%= user.address %></td>
    <td><%= user.city %></td>
    <td><%= user.state %></td>
    <td><%= user.zip %></td>
    <td><%= user.website %></td>
    <td><%= user.phone %></td>
    <td><%= user.about %></td>
    <td><%= user.business %></td>
    <td><%= user.tag %></td>
    <!-- <td><#%= user.favorite %></td> -->
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
